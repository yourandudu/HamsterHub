<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Data extraction rules for Android 12+ -->
    <data-extraction-rules>
        <cloud-backup>
            <exclude domain="sharedpref" path="FlutterSecureStorage"/>
            <exclude domain="database" path="tryon.db"/>
        </cloud-backup>
        <device-transfer>
            <exclude domain="sharedpref" path="FlutterSecureStorage"/>
        </device-transfer>
    </data-extraction-rules>
</resources>